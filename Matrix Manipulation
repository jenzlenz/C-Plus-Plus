/*
Synopsis: to manipulate a matrix via pointers
*/

#include <iostream>
using namespace std;

#include <stdlib.h> //for the seed and random number generator

#define R 10 //number of rows
#define C 8 // number of columns

void Matrix_Gen(char* Matrix_ptr[], int row, int col);
void print_Matrix(char* Matrix_ptr[], int row, int col);
void print_Matrix2(char** Matrix_ptr_ptr, int row, int col);

int main()
{

        char Matrix[R][C] = {'\0'};
        char *Matrix_ptr[R] = {NULL};

        char **Matrix_ptr_ptr = NULL; //char ptr to the array of char ptrs
        Matrix_ptr_ptr = &Matrix_ptr[0];

        unsigned long int seed = 1023;
        srand(seed); //initializes the random number generator

        //assign pointers to rows
        for(int i = 0; i < R; ++i)
                Matrix_ptr[i] = &Matrix[i][0]; //zeroth column

        //populate matrix via array of pointers
        Matrix_Gen(Matrix_ptr, R, C); //passing a ptr to an array of ptrs

        print_Matrix(Matrix_ptr, R, C);

        print_Matrix2(Matrix_ptr_ptr, R, C);

return 0;
}

void Matrix_Gen(char* Matrix_ptr[], int row, int col)
{
        cout << "Inside the matrx gen function" << endl;
        for(int i = 0; i < row; ++i)
          for(int j = 0; j < col; ++j)
            *(Matrix_ptr[i]+j) = 65 + (rand() % 30);  //will produce numbers < 100

}

void print_Matrix(char* Matrix_ptr[], int row, int col)
{
